{
  "name": "eslint-plugin-sanity",
  "version": "0.0.1",
  "description": "ESLint plugin for Sanity projects - lint GROQ queries and schemas",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@sanity/groq-lint": "workspace:*"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/types": "^8.0.0",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": ["eslint", "eslint-plugin", "sanity", "groq"]
}
