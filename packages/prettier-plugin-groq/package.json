{
  "name": "@sanity-labs/prettier-plugin-groq",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Prettier plugin for formatting GROQ queries",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanity-labs/sanity-lint.git",
    "directory": "packages/prettier-plugin-groq"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./embed": {
      "types": "./dist/embed.d.ts",
      "import": "./dist/embed.js",
      "require": "./dist/embed.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@sanity-labs/groq-wasm": "workspace:*",
    "groq-js": "^1.14.0"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@sanity-labs/lint-core": "workspace:*",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "sanity",
    "groq",
    "prettier",
    "prettier-plugin",
    "formatter"
  ]
}
